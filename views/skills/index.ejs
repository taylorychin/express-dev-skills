<%- include("../partials/header") %>

    <h1>Skills</h1>
    <ul>
        <% skills.forEach(function(s) { %>
            <li>
                <form action="/skills/<%= s.id %>?_method=DELETE" class="delete=form" method="POST">
                    <button type="submit">X</button>
                </form>
                <a href="/skills/<%= s.id %>">
                    <%= s.skillName %>
                </a>
            </li>
            <% }); %>
    </ul>
    <a href="skills/new">Add Skill</a>
    <%- include("../partials/footer") %>